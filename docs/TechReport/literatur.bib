  title = {Comparison of user mobility pattern prediction algorithms to increase handover trigger accuracy},
  booktitle = {The 63rd IEEE Vehicular Technology Conference 2006},
  month = {May},
  pages = {952-956},
  publisher = {IEEE},
  volume = {2},
  year = {2006},
  ID = {d4t90qlp1c},
  ABSTRACT = {The estimation of correct triggers for handover in cellular networks belongs to the critical tasks for accurate network operation. The importance of seamless handover even rises according to the increasing number of available radio access technologies, demanding for a reliable vertical handover. To aid the handover process, mobility prediction technologies gain interest and provide the possibility to prepare for handover in advance. The approaches presented here feature prediction of macro-mobility as an additional measure to aid handover decisions. The prediction is based on statistical data gained from the observation of movement across multiple cells. One of the main features lies in a generic user centric calculation of the most likely next hop during user movement, compared to network specific technological methods.},
  LS8AUTHOREN = {michaelis}
}


@inproceedings{Michaelis/Wietfeld/2006b,
  address = {Athens, Greece},
  author = {Michaelis , S. and Wietfeld , C.},
  title = {Evaluation and comparison of prediction stability for user movement pattern detection algorithms},
  booktitle = {12th European Wireless Conference},
  month = {April},
  pages = {0},
  publisher = {VDE Verlag},
  year = {2006},
  ID = {d4t90qhkhs},
  ABSTRACT = {With increasing availability of high bandwidth mobile networks, ranging from classical UMTS up to local IEEE 802.16 cells, efficient management for mobile resources gains additional significance. Approaches targeting at detecting repeating patterns in mobile user movements enable network operators to react to cell handovers and bottle necks in advance. This paper discusses several methods to extract movement patterns from mobile users and analyses sensitiveness to mobility data and algorithm parametrisation.},
  LS8AUTHOREN = {michaelis}
}


@inproceedings{Michaelis/etal/2008a,
  address = {Reykjavik},
  author = {Michaelis , S. and Lewandowski , A. and Daniel , K. and Wietfeld , C.},
  title = {Macromobility Prognosis for high-priority Resource Reservation in Wireless Networks},
  booktitle = {5th IEEE International Symposium on Wireless Communication Systems (ISWCS)},
  month = {October},
  pages = {184-188},
  publisher = {IEEE},
  year = {2008},
  ID = {glm8f303},
  ABSTRACT = {Efficient management of mobile network resources is a critical task for successful operation. Giving higher priority and Quality of Service to applications with a high return on invest value, demands for intelligent distribution of bit rates and radio access. The research approach proposed here is based on the fact, that users cannot move freely, but are restricted to streets or rails in position, direction and speed. Following the geographical topology while in motion some locations generate a unique signature by forcing the user to traverse similar sequences of base stations. The signatures can be revealed by applying pattern detection methods on the historical user movements, allowing to predict future positions of the user on a large time scale, especially to reserve resources for rescue workers. In this paper we discuss the effect of different attributes about the user\'s movement for the prediction quality on different pattern detection algorithms in order to improve and accelerate the process of rescue missions. Further the availability of these attributes will be discussed for different scenarios and validated by traces from an actual large scale HSDPA/GSM network.},
  LS8AUTHOREN = {michaelis}
}


