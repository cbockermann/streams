<!-- 
    This simple container experiment checks whether the ServiceInjection is working properly.
 -->

<Container>

	<Stream id="input" class="stream.io.TimeStream" limit = "100000">
		
	</Stream>
	
	
	<Service id = "graph" class="stream.data.graph.GraphService" file="/tmp/traffic-graph.csv" />


	<Process input="input" multiply="100">

	<!-- 
		<stream.flow.Delay time="10ms" />
	 -->
		<stream.test.LogData message="Process: ${process.id}   %{data.@id}" />
		
	</Process>
	
</Container>