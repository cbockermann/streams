<container address="127.0.0.1" port="9109">

    <stream id="twitter" class="stream.io.JSONStream" limit="1000"
    	url="http://kirmes.cs.uni-dortmund.de/data/twitter.json" />

    <process input="twitter">
    	<PrintData />
		<stream.flow.Delay time="1second" />
		<stream.logger.Message message="Sending data to kirmes/twitter" />
		<stream.flow.Enqueue queue="//kirmes/twitter" />
    </process>

</container>
