@techreport{Dunne/etal/2002a,
  address = {Dublin, Ireland},
  author = {Dunne, Kevin and Cunningham, Padraig and Azuaje, Francisco},
  title = {Solutions to Instability Problems with Sequential Wrapper-BasedApproaches To Feature Selection},
  institution = {Department of Computer Science, Trinity College},
