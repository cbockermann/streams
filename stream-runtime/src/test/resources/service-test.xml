<!-- 
    This simple container experiment checks whether the ServiceInjection is working properly.
 -->

<Container import="stream.learner">

	<Stream id="input" class="stream.test.TestStream" limit = "10" />

	<Process input="input">
	
		<stream.test.CountServiceLearner id="metaData" />
		
		<stream.test.CountCheck countService-ref="metaData" />

		<stream.test.LogData />
		
	</Process>
	
</Container>