<module>
    
    <queue id="output.module${queuecopy.id}" capacity="${join.queuecapacity${include.id}}" queuecopies="[${join.copies${include.id}}]:[${include.copies}]"/>
    
	<process input="output.module${copy.id}" copies="[${join.copies${include.id}}]:[${include.copies}]" >
		<stream.flow.Emitter sink="input.module${include.id}:${queuecopy.id}" queuecopies="${include.copies}" />
	</process>
	
    <queue id="input.module${include.id}:${queuecopy.id}" capacity="${join.queuecapacity${include.id}}" queuecopies="${include.copies}" />
     
</module>