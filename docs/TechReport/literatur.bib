@article{Marwedel/2007a,
  address = {l'Aquila},
  author = {Marwedel, Peter},
  title = {Memory architecture aware compilation (tutorial)},
  journal = {\url{http://ls12-www.cs.tu-dortmund.de/publications/papers/2007-marwedel-acaces.zip}},
  month = {July},
  note = {Tutorial at the hipeac ACACES summer school at l'Aquila.},
  year = {2007},
  ID = {d71h66v1mo},
  ABSTRACT = {The speed of processors is currently improving at a much faster rate    than the speed of memories. It has been found that the speed gap    between processors and memories is doubling within at most two years    for many scenarios. This means that, in our quest for speed, we will    hit the memory wall, a situation in which the system speed is limited    by the speed of a relatively slow memory. This speed gap was initially    important just for mainframe and desktop computing but is now also    relevant for embedded devices. Memory hierarchies are seen as the    key approach towards easing the problem. However, cache-based hierarchies    are not energy-efficient and may have a poor timing-predictability.    In the tutorial, we show how the energy efficiency and timing-predictability    can be improved by using software which has been optimized for a    particular memory architecture. The focus is on small scratch-pad    memories (called tightly coupled memories by ARM) and the compiler-based    mapping of hot-spots of applications to these memories. For these    approaches, the compiler needs to be aware of the memory architecture.    We compare static approaches for such a mapping with approaches based    on dynamic overlays managed by the compiler. We also show how the    concept can be applied in multi-threaded and multi-processor environments.    Required optimizations can be added to existing tool chains. The    concepts lead to dramatic improvements in terms of the energy consumption    and timing predictability. Furthermore, the tutorial also covers    some optimizations for caches and briefly puts optimizations for    other levels of the memory hierarchy into perspective.}
}


