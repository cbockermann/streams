<container id="mem-log" server="true" address="192.168.128.4" port="0">
   <stream id="time" class="stream.io.TimeStream" gap="1 second" />

   <process input="time">
       <PrintData />
       <stream.flow.Delay time="1 second" />

   </process>

   <Monitor interval="1 second">
       <stream.monitor.MemoryUsage />
       <PrintData />
   </Monitor>

</container>
